{
    "name": "hakito/cakephp-mailqueue-plugin",
    "description": "Plugin to store mail in a queue for later sendout",
    "keywords": ["cakephp", "email", "queue"],
    "type": "cakephp-plugin",
    "license": "GPL-2.0",
    "homepage": "https://github.com/hakito/CakePHP-Mailqueue-Plugin",
    "authors": [
        {
            "name": "Gerd Katzenbeisser",
            "homepage": "https://github.com/hakito/CakePHP-Mailqueue-Plugin/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "MailQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MailQueue\\Test\\": "tests/"
        }
    },
    "require": {
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "extra": {
        "installer-name": "MailQueue"
    }
}
